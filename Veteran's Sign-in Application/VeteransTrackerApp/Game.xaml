        <TextBox x:Name="Out6" HorizontalAlignment="Left" Height="75" TextWrapping="Wrap" Text=" X" VerticalAlignment="Top" Width="75" Margin="10,170,0,0" FontSize="48" Visibility="Hidden"/>

