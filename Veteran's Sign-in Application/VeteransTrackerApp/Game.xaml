        <Image Name ="EndGame" HorizontalAlignment="Left" Height="235" VerticalAlignment="Top" Width="235" Margin="10,10,0,0"/>
        <TextBox Name ="EndGame2" HorizontalAlignment="Left" Height="235" TextWrapping="Wrap" Text="Player 2 is the winner" VerticalAlignment="Top" Width="235" Margin="10,10,0,0" Background="Black" Foreground="Green" FontFamily="SimSun" FontSize="55" Visibility="Hidden"/>
        <TextBox Name="EndGameT" Visibility="Hidden" Background ="Black" Foreground="Green" HorizontalAlignment="Left" Height="212" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="144" Margin="250,33,0,0"/>
